<!--pages/calendar/calendar.wxml-->
<view class="page">
  <view class="calendar">
    <!--https://github.com/AFAP/XCX-Plugins-->
    <calendar  selected-date='{{selectedDate}}' bindOnDayClick="onDayClick" bindOnMonthChange="onMonthChange" show-more-days="{{false}}"  />
  </view>
</view>
<text>{{selectedDate}}</text>
